vlog -vlog01compat -work work +incdir+D:/FilterVerilog/Chapter_7/E7_1/MisMatch/simulation/modelsim {D:/FilterVerilog/Chapter_7/E7_1/MisMatch/simulation/modelsim/MisMatch.vt}

vsim -t 1ps -L altera_ver -L lpm_ver -L sgate_ver -L altera_mf_ver -L altera_lnsim_ver -L cycloneive_ver -L rtl_work -L work -voptargs="+acc"  MisMatch_vlg_tst

